
@{
    ViewBag.Title = "Notification";
    Layout = "~/Views/Shared/_SellerLayout.cshtml";
}

<link href="~/css/SellerStyle/NotificationStyle.css" rel="stylesheet" />
@{
    var grid = new WebGrid(source: @ViewBag.orderlist, canPage: true, rowsPerPage: 5, selectionFieldName: "selectedRow");
}
@grid.GetHtml(
         tableStyle: "webGrid", headerStyle: "header", alternatingRowStyle: "altColor", selectedRowStyle: "clickable",

    columns: new[]
    {


        grid.Column(columnName:"ServiceName", header: "Service Name"),
          grid.Column(columnName:"ProductName", header: "Product Name"),
           grid.Column(columnName:"UserName", header: "Buyer")

        }
        )


